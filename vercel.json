{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/bmicalc/assets/(.*)",
        "dest": "/dist/assets/$1"
      },
      {
        "src": "/bmicalc/(.*)",
        "dest": "/dist/index.html"
      }
    ]
  }
  